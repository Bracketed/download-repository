{
	"name": "@bracketed/gitdownloader",
	"author": "@bracketed",
	"version": "1.0.4",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"publishConfig": {
		"access": "public"
	},
	"homepage": "https://bracketed.co.uk",
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ninjaninja140/download-repository.git",
		"directory": "."
	},
	"bugs": {
		"url": "https://github.com/ninjaninja140/download-repository/issues"
	},
	"scripts": {
		"compile": "yarn tsc",
		"lint": "yarn prettier --write src/**/*.ts",
		"validate": "yarn prettier --check src/**/*.ts",
		"lint:all": "yarn prettier --write .",
		"validate:all": "yarn prettier --check ."
	},
	"keywords": [
		"bracketed",
		"typescript",
		"ts",
		"yarn",
		"github",
		"git-clone",
		"download"
	],
	"description": "Download and extract a git repository using node!",
	"dependencies": {
		"download": "^8.0.0"
	},
	"devDependencies": {
		"@bracketed/typescript-config": "^4.4.0",
		"@types/download": "^8.0.5",
		"prettier": "^3.1.1",
		"typescript": "^5.3.3"
	},
	"packageManager": "yarn@4.0.2"
}
